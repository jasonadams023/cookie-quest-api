{
    "name": "cookie-quest",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "start": "node ./app/server",
        "start:watch": "nodemon ./app/server",
        "test": "jest",
        "test:watch": "jest --watch",
        "build": "npm run build:base && npm run build:dev",
        "build:base": "docker build -t cookie-quest-api:base . -f ./docker/base/Dockerfile",
        "build:dev": "docker build -t cookie-quest-api:dev . -f ./docker/dev/Dockerfile",
        "compose": "docker-compose up -d",
        "compose:down": "docker-compose down",
        "compose:dev": "docker-compose -f docker-compose.yml -f docker-compose.dev.yml up -d"
    },
    "devDependencies": {
        "jest": "^22.4.3",
        "nodemon": "^1.17.3",
        "supertest": "^3.0.0"
    },
    "dependencies": {
        "node": "^9.11.0"
    }
}
