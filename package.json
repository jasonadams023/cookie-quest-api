{
    "name": "cookie-quest",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "start": "node ./app/server",
        "start:watch": "nodemon -q ./app/server",
        "start:docker": "docker-compose up",
        "start:docker:dev": "docker-compose -f docker-compose.yml -f docker-compose.dev.yml up",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:watchAll": "jest --watchAll",
        "test:container": "docker exec -it $(docker container ls -f \"volume=/cookie-quest\" -qn 1) npm run test:watchAll",
        "build": "npm run build:base && npm run build:dev",
        "build:base": "docker build -t cookie-quest-api:base . -f ./docker/base/Dockerfile",
        "build:dev": "docker build -t cookie-quest-api:dev . -f ./docker/dev/Dockerfile",
        "compose": "docker-compose up -d",
        "compose:down": "docker-compose down",
        "compose:dev": "docker-compose -f docker-compose.yml -f docker-compose.dev.yml up -d"
    },
    "devDependencies": {
        "jest": "^22.4.3",
        "nodemon": "^1.17.3",
        "supertest": "^3.0.0"
    },
    "engines": {
        "node": "9.11.1",
        "npm": "5.6.0"
    }
}
